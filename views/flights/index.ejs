<%- include('../partials/header') %>
    <h2>views/flights/index</h2>
    <table id="list">
        <thead>
          <tr>
            <th>Airline</th>
            <th>Airport</th>
            <th>Flight Number</th>
          </tr>
        </thead>
        <tbody>
          <% flights.forEach(function(f) { %>
            <tr>
              <td><%= f.arline %></td>
              <td><%= f.airport %></td>
              <td><a href="/flights/<%= f._id %>">DETAILS</a></td>
            </tr>
            <% }); %>
          </tbody>
        </table>
<%- include('../partials/footer') %>